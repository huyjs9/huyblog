{
  "private": true,
  "name": "minimal-blog",
  "description": "Typography driven, feature-rich blogging theme with minimal aesthetics. Includes tags/categories support and extensive features for code blocks such as live preview, line numbers, and line highlighting.",
  "version": "1.2.0",
  "author": "Huy Bui <huybt389@gmail.com>",
  "license": "MIT",
  "starter-name": "gatsby-starter-minimal-blog",
  "scripts": {
    "build": "gatsby clean && gatsby build",
    "develop": "gatsby develop",
    "start": "rm -rf .cache && gatsby develop",
    "develop:cypress": "cross-env CYPRESS_SUPPORT=y npm run develop",
    "build:cypress": "cross-env CYPRESS_SUPPORT=y npm run build",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "cy:open": "cypress open",
    "cy:run": "cypress run --headed"
  },
  "dependencies": {
    "@docsearch/react": "^3.2.1",
    "@lekoarts/gatsby-theme-minimal-blog": "^4.0.0",
    "@loadable/component": "^5.15.2",
    "gatsby": "^4.18.2",
    "gatsby-image": "^3.11.0",
    "gatsby-omni-font-loader": "^2.0.0",
    "gatsby-plugin-canonical-urls": "^4.21.0",
    "gatsby-plugin-feed": "^4.18.1",
    "gatsby-plugin-gatsby-cloud": "^4.18.1",
    "gatsby-plugin-image": "^2.21.0",
    "gatsby-plugin-manifest": "^4.18.1",
    "gatsby-plugin-offline": "^5.21.0",
    "gatsby-plugin-scroll-indicator": "^1.0.1",
    "gatsby-plugin-scroll-reveal": "^0.0.7",
    "gatsby-plugin-sharp": "^4.21.0",
    "gatsby-plugin-sitemap": "^5.18.1",
    "gatsby-plugin-vercel": "^1.0.3",
    "gatsby-plugin-webpack-bundle-analyser-v2": "^1.1.27",
    "gatsby-remark-autolink-headers": "^5.21.0",
    "gatsby-remark-external-links": "^0.0.4",
    "gatsby-transformer-remark": "^5.21.0",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "react-gif-player": "^0.4.2",
    "typescript": "^4.8.2"
  },
  "devDependencies": {
    "cross-env": "^7.0.3"
  },
  "prettier": {
    "printWidth": 77,
    "tabWidth": 2,
    "singleQuote": true,
    "semi": false
  }
}